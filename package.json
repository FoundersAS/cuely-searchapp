{
  "name": "cuely-search",
  "version": "1.3.0",
  "description": "A desktop app for searching via Cuely",
  "main": "index.js",
  "productName": "Cuely",
  "author": "Pipetop Aps",
  "scripts": {
    "start": "NODE_ENV=development electron .",
    "dev": "concurrently -k 'babel-node server.js' 'BACKEND=development npm start'",
    "dev:prod": "concurrently -k 'babel-node server.js' 'BACKEND=production npm start'",
    "clean": "rm -rf ./dist ./Cuely-*",
    "build": "npm run clean && webpack --config webpack.config.production.js && electron-packager . Cuely --platform=darwin --arch=all --app-bundle-id=com.cuely.search --app-version=$npm_package_version --build-version='1.0.100' --prune --overwrite --icon ./cuely_icon --osx-sign.identity=17387061A6C06605513BCFC8123810ADC169ED18",
    "build:zip": "npm run build && zip -r -y -9 ./Cuely-darwin-x64/cuely-$npm_package_version-osx.zip ./Cuely-darwin-x64/Cuely.app",
    "deploy:version": "echo $npm_package_version > ./Cuely-darwin-x64/VERSION && aws s3 cp ./Cuely-darwin-x64/VERSION s3://updates.cuely.co/releases/osx/",
    "deploy": "npm run build:zip && npm run deploy:version && aws s3 cp ./Cuely-darwin-x64/cuely-$npm_package_version-osx.zip s3://updates.cuely.co/releases/osx/"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pipetop/cuely-searchapp.git"
  },
  "keywords": [
    "Electron",
    "cuely",
    "search"
  ],
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/pipetop/cuely-searchapp/issues"
  },
  "homepage": "https://github.com/electron/electron-quick-start#readme",
  "dependencies": {
    "algoliasearch": "^3.18.1",
    "analytics-node": "^2.1.0",
    "auto-launch": "^5.0.1",
    "babel-core": "^6.14.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babyparse": "^0.4.6",
    "electron": "^1.4.6",
    "markdown-it": "^8.0.1",
    "marked": "^0.3.6",
    "mathjs": "^3.8.0",
    "mdfind": "^1.0.0",
    "moment": "^2.15.1",
    "opbeat": "^3.21.1",
    "plist": "^2.0.1",
    "react": "^15.3.2",
    "react-custom-scrollbars": "^4.0.0",
    "react-dom": "^15.3.2",
    "react-markdown": "^2.4.2",
    "showdown": "^1.4.3",
    "simple-plist": "^0.2.1",
    "superagent": "^2.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-loader": "^6.2.5",
    "concurrently": "^3.0.0",
    "css-loader": "^0.25.0",
    "electron-packager": "^8.2.0",
    "express": "^4.14.0",
    "file-loader": "^0.9.0",
    "node-sass": "^3.11.1",
    "postcss-loader": "^0.13.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.3",
    "webpack-dev-middleware": "^1.8.3",
    "webpack-hot-middleware": "^2.12.2",
    "webpack-target-electron-renderer": "^0.4.0"
  }
}
